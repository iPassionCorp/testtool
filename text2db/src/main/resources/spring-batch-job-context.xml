	<import resource="classpath:spring-batch-context.xml" />

	<!-- Jobs -->

	<batch:job id="importCompensateidJob">
		<batch:step id="compensateidStep1">
			<batch:tasklet transaction-manager="transactionManager">
				<batch:chunk reader="compensateidItemReader" writer="tlitextDatabaseItemWriter"
					commit-interval="10000" skip-limit="630">
					<batch:skippable-exception-classes>
						<batch:include
							class="org.springframework.batch.item.file.transform.IncorrectLineLengthException" />
					</batch:skippable-exception-classes>
				</batch:chunk>
			</batch:tasklet>
		</batch:step>
		<batch:listeners>
			<batch:listener ref="jobListener" />
		</batch:listeners>
	</batch:job>

