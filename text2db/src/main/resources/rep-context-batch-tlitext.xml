    <bean id="repTlitextCertReader" class="org.springframework.batch.item.database.JdbcCursorItemReader">
        <property name="dataSource" ref="dataSource"/>
        <property name="sql">
            <value>
                <![CDATA[
                    SELECT * FROM dm.cert
                ]]>
            </value>
        </property>
        <property name="rowMapper">
            <bean class="org.springframework.jdbc.core.BeanPropertyRowMapper">
                <property name="mappedClass" value="th.co.ipassion.testtool.dto.tlitext.Cert"/>
            </bean>
        </property>
    </bean>

